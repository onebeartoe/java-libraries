<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="java-libraries-test-suite" parallel="none">
    <test name="unit-tests">
        <groups>
            <run>
                <include name="unit" />
                <include name="smoke" />
                <exclude name="regression"/>
            </run>
        </groups>

        <packages>    
            <package name="org.onebeartoe.filesystem"/>
        </packages>
    </test>    
</suite>
